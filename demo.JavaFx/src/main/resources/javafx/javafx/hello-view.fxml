<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER_LEFT" prefHeight="355.0" prefWidth="451.0" spacing="20.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafx.javafx.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Label text="Username:" />
    <TextField fx:id="txtUsername" prefHeight="31.0" prefWidth="381.0" />
    <Label text="Password:" />
    <PasswordField fx:id="txtPassword" prefHeight="31.0" prefWidth="411.0" />

    <Label fx:id="welcomeText" />
    
    <Button fx:id="btnLogin" onAction="#onHelloButtonClick" prefHeight="35.0" prefWidth="58.0" text="Login" />
</VBox>
